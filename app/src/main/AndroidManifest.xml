<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.souttab.budayalampung">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    
    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:uiOptions="splitActionBarWhenNarrow">
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.Sherlock.Light">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.souttab.budayalampung.detail.DetailFragmentKesenian"
            android:theme="@style/Theme.Sherlock.Light" />
        <activity
            android:name="com.souttab.budayalampung.detail.DetailFragmentMakanan"
            android:theme="@style/Theme.Sherlock.Light"/>
        <activity
            android:name="com.souttab.budayalampung.detail.DetailFragmentAdatIstiadat"
            android:theme="@style/Theme.Sherlock.Light"/>
        <activity
            android:name="com.souttab.budayalampung.detail.DetailFragmentBahasa"
            android:theme="@style/Theme.Sherlock.Light" />

    </application>

</manifest>
